<% @title=": About Us" %>


<script type="text/javascript">
    mixpanel.track("about_us", {

    });
</script>





<body style="background-color:#eee;">

<div class="header dashboard_header">
  <div class="dashboard-header-icon-block"><%= image_tag("flying_pig_header.png", class:"dashboard-header-icon") %> </div>
  <div class="dashboard-header-logo-block"> <%= image_tag("savvysaverlogo.png", class:"dashboard-header-logo") %></div>
    <br />
  </div>
</div>

<div class="signup-buttons-block">
    
      <div class="signup-button">
        <%= link_to customer_facebook_omniauth_authorize_path do %>
        <div class="fb-button">
          <div class="fb-icon-box">
          <%= image_tag("fb_button_icon_blue.png", class:"fb_signin_icon") %>
          </div>
          <span class="signin-button-text">Connect with Facebook</span>
        </div>
        <% end %>
      </div>

      <div class="signup-button">
        <%= link_to customer_google_oauth2_omniauth_authorize_path do %>
        <div class="google-button">
          <div class="google-icon-box">
            <%= image_tag("google_button_icon", class:"fb_signin_icon") %>
          </div>
          <span class="signin-button-text" style="margin-left:32px; float:left;">Connect with Google</span>
        </div>
        <% end %>
      </div>
</div>



</body>